{
  "name": "demyashev/php.wow-srp6",
  "version": "0.0.2",
  "type": "library",
  "description": "A PHP library for generating authorization keys and encrypting data for World of Warcraft",
  "keywords": ["srp", "srp6", "wow", "php", "php84", "wotlk", "3.3.5a"],
  "homepage": "https://github.com/demyashev/php.wow-srp6",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Demyashev",
      "email": "alexander.demyashev@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ext-gmp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "WOWSRP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@composer install",
      "phpunit"
    ]
  }
}
